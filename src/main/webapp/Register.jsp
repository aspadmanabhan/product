<div class="form-group">
<body>

<div class="container">
  <h2>Registration Form</h2>
  <form class="form-horizontal" role="form" method="post">
    <div class="form-group">
      <label class="control-label col-sm-2" for="user">Enter Name</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="user" placeholder="Enter email">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="eml">Enter Email id</label>
      <div class="col-sm-10">
        <input type="User Name" class="form-control" id="eml" placeholder="Enter Email Id">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="pass">Enter Password</label>
      <div class="col-sm-10">
        <input type="Password" class="form-control" id="pass" placeholder="Enter Email Id">
      </div>
    </div>
    
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
        <button type="submit" class="btn btn-default">Submit</button>
      </div>
    </div>
  </form>
</div>

</body>
